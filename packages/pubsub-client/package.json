{
  "name": "@saga-ed/soa-pubsub-client",
  "version": "1.0.0",
  "description": "Client library for pubsub functionality using tRPC",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "check-types": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:integration": "vitest run __tests__/integration",
    "test:unit": "vitest run __tests__/unit"
  },
  "dependencies": {
    "@saga-ed/soa-logger": "workspace:^",
    "@saga-ed/soa-pubsub-core": "workspace:*",
    "@saga-ed/soa-pubsub-server": "workspace:^",
    "@trpc/client": "^11.0.0",
    "@trpc/server": "^11.4.3",
    "express": "^4.18.2",
    "inversify": "^6.2.2",
    "zod": "3.25.67"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^24.0.3",
    "@types/supertest": "^6.0.3",
    "supertest": "^7.1.1",
    "tsup": "^8.5.0",
    "typescript": "^5.8.2",
    "vitest": "^1.5.0"
  },
  "peerDependencies": {
    "@trpc/client": "^11.0.0"
  },
  "keywords": [
    "pubsub",
    "trpc",
    "events",
    "real-time",
    "typescript"
  ],
  "author": "Saga SOA Team",
  "license": "MIT"
}
